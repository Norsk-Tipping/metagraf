{
  "kind": "MetaGraf",
  "version": "v0alpha1",
  "metadata": {
    "name": "ExampleComponent",
    "version": "1.0.0",
    "description": "Example Component, does nothing!",
    "labels": {
    },
    "annotations": {
      "norsk-tipping.no/component_type": "frontend",
      "norsk-tipping.no/exposable": "true",
      "norsk-tipping.no/appcode": "123",
      "norsk-tipping.no/tags": "nothing"
    }
  },
  "spec": {
    "resources": [
      {
        "name": "SomeResource",
        "type": "service",
        "version": "4.3.3",
        "match": ">=",
        "required": "true"
      }
    ],
    "environment": {
      "local": [
        {
          "name": "LocalVariableName",
          "required": "true",
          "type": "string",
          "description": "Some local variable name description"
        }
      ],
      "external": {
        "consumes": [
          {
            "name": "VariableName",
            "required": "true",
            "description": "Some external variable description"
          }
        ],
        "introduces": []
      }
    },
    "config": [
      {
        "name": "Configuration option",
        "required": "false",
        "description": "This configuration option does less than nothing",
        "type": "boolean",
        "default": "false"
      }
    ]
  }
}