{
  "kind": "MetaGraf",
  "version": "v0alpha1",
  "metadata": {
    "name": "Payment",
    "version": "4.1.9",
    "description": "Payment is a RESTfull web service for doing payment, responsible gaming verification and logging for later reconciliation.",
    "labels": {
    },
    "annotations": {
      "norsk-tipping.no/component_type": "frontend",
      "norsk-tipping.no/exposable": "true",
      "norsk-tipping.no/appcode": "123",
      "norsk-tipping.no/tags": "id, betaling"
    }
  },
  "spec": {
    "resources": [
      {
        "name": "GetEnvironmentProperties",
        "type": "service",
        "version": "4.3.3",
        "match": ">=",
        "required": "true"
      },
      {
        "name": "Session",
        "type": "service",
        "version": "4.3.0",
        "match": ">=",
        "required": "true"
      },
      {
        "name": "GetBPCardUserInformation",
        "type": "service",
        "version": "3.4.1",
        "match": ">=",
        "required": "true"
      },
      {
        "name": "DoPayment",
        "type": "service",
        "version": "4.2.3",
        "match": ">=",
        "required": "true"
      }
    ],
    "environment": {
      "local": [
        {
          "name": "GetEnvironmentPropertiesURL",
          "required": "true",
          "type": "string",
          "description": "URL til GetEnviromentProperties tjeneste"
        },
        {
          "name": "EnvironmentName",
          "required": "true",
          "type": "string",
          "description": "Name of environment"
        }
      ],
      "external": {
        "consumes": [
          {
            "name": "SessionURL",
            "required": "true",
            "description": "URL to Session service"
          },
          {
            "name": "DoPaymentURL",
            "required": "true",
            "description": "URL to DoPayment service"
          },
          {
            "name": "GetBPCardUserInformationURL",
            "required": "true",
            "description": "URL to GetBPCardUserInformation service"
          }
        ],
        "introduces": []
      }
    },
    "config": [
      {
        "name": "Payment.LogRequestsAndResponses",
        "required": "false",
        "description": "For troubleshooting, Enable the Jersey LoggingFilter to log http requests to Payment and responses from Payment (not other sub-services). NB: This property is only read during application startup.",
        "type": "boolean",
        "default": "false"
      },
      {
        "name": "Payment.SocketTimeout",
        "required": "false"
      }
    ]
  }
}