digraph {
  rankdir=tplr
  node [color=grey25,
        fillcolor=wheat,
        shape=box,
        style="rounded,filled"];
{{ range $mg := . -}}
"{{ $mg.Metadata.Name -}}-v{{$mg.Metadata.Version}}"
{{ range $mg.Spec.Resources -}}
"{{ $mg.Metadata.Name}}-v{{$mg.Metadata.Version}}" -> "{{ .Name }}-v{{.Version}}"
{{ end -}}
{{ end -}}
}